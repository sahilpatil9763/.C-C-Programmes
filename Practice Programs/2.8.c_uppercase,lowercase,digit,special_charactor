#include<stdio.h>
#include<conio.h>
int main()
{ char ch;
 printf("\n enter any key:");
 scanf("%c",&ch);
if((ch>='A' && ch<='Z')||(ch>='a' && ch<='z'))
{
  printf("\n '%c' is alphabet",ch);
}
 else if(ch>='0' && ch<='9')
{
  printf("\n '%c' is digit",ch);
}
 else
{ 
 printf("\n '%c' is special charactor",ch);
}

getch();
return 0;
}
