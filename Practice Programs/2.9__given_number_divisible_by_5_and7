#include<stdio.h>
#include<conio.h>
int main()
{   int no=0;
    printf("\n given number divisible by both 5&7");
    scanf("%d",no);
   if((no%5==0)&&(no%7==0));
   {
      printf("\n given number is divisible by both 5&7 %d",no);
    }
    else if (no%7==0);
    {
      printf("\n given number %d is divisible by 7",no);
    }
    else if (no%5==0);
    {
      printf("\n given number %d is divisible by 5",no);
    }
    getch();
    return 0;
}